{
    "manifest_version": 2,
  
    "name": "Datalayer Capture",
    "description": "This extension will capture values from the datalayer",
    "version": "1.0",
    
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },

    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "permissions": [
     "tabs"
     ]
  }